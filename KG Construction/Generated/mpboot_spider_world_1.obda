[PrefixDeclaration]
obda:		https://w3id.org/obda/vocabulary#
xsd:		http://www.w3.org/2001/XMLSchema#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	BOOTSTRAPPED-MAPPING-ID1
target		<http://valuenet/ontop/country/code={code}> a <http://valuenet/ontop/country> ; <http://valuenet/ontop/country#code> {code}^^xsd:string ; <http://valuenet/ontop/country#name> {name}^^xsd:string ; <http://valuenet/ontop/country#continent> {continent}^^xsd:string ; <http://valuenet/ontop/country#region> {region}^^xsd:string ; <http://valuenet/ontop/country#surfacearea> {surfacearea}^^xsd:decimal ; <http://valuenet/ontop/country#indepyear> {indepyear}^^xsd:integer ; <http://valuenet/ontop/country#population> {population}^^xsd:integer ; <http://valuenet/ontop/country#lifeexpectancy> {lifeexpectancy}^^xsd:decimal ; <http://valuenet/ontop/country#gnp> {gnp}^^xsd:decimal ; <http://valuenet/ontop/country#gnpold> {gnpold}^^xsd:decimal ; <http://valuenet/ontop/country#localname> {localname}^^xsd:string ; <http://valuenet/ontop/country#governmentform> {governmentform}^^xsd:string ; <http://valuenet/ontop/country#headofstate> {headofstate}^^xsd:string ; <http://valuenet/ontop/country#capital> {capital}^^xsd:integer ; <http://valuenet/ontop/country#code2> {code2}^^xsd:string . 
source		SELECT * FROM "country"

mappingId	BOOTSTRAPPED-MAPPING-ID2
target		<http://valuenet/ontop/city/id={id}> a <http://valuenet/ontop/city> ; <http://valuenet/ontop/city#id> {id}^^xsd:integer ; <http://valuenet/ontop/city#name> {name}^^xsd:string ; <http://valuenet/ontop/city#countrycode> {countrycode}^^xsd:string ; <http://valuenet/ontop/city#district> {district}^^xsd:string ; <http://valuenet/ontop/city#population> {population}^^xsd:integer . 
source		SELECT * FROM "city"

mappingId	BOOTSTRAPPED-MAPPING-ID3
target		<http://valuenet/ontop/city/id={city_id}> <http://valuenet/ontop/city#ref-countrycode> <http://valuenet/ontop/country/code={country_code}> . 
source		SELECT "city"."id" AS city_id, "country"."code" AS country_code FROM "city", "country" WHERE "city"."countrycode" = "country"."code"

mappingId	BOOTSTRAPPED-MAPPING-ID4
target		<http://valuenet/ontop/countrylanguage/countrycode={countrycode};language={language}> a <http://valuenet/ontop/countrylanguage> ; <http://valuenet/ontop/countrylanguage#countrycode> {countrycode}^^xsd:string ; <http://valuenet/ontop/countrylanguage#language> {language}^^xsd:string ; <http://valuenet/ontop/countrylanguage#isofficial> {isofficial}^^xsd:string ; <http://valuenet/ontop/countrylanguage#percentage> {percentage}^^xsd:decimal . 
source		SELECT * FROM "countrylanguage"

mappingId	BOOTSTRAPPED-MAPPING-ID5
target		<http://valuenet/ontop/countrylanguage/countrycode={countrylanguage_countrycode};language={countrylanguage_language}> <http://valuenet/ontop/countrylanguage#ref-countrycode> <http://valuenet/ontop/country/code={country_code}> . 
source		SELECT "countrylanguage"."countrycode" AS countrylanguage_countrycode, "countrylanguage"."language" AS countrylanguage_language, "country"."code" AS country_code FROM "countrylanguage", "country" WHERE "countrylanguage"."countrycode" = "country"."code"
]]

