[PrefixDeclaration]
obda:		https://w3id.org/obda/vocabulary#
xsd:		http://www.w3.org/2001/XMLSchema#
owl:		http://www.w3.org/2002/07/owl#
rdf:		http://www.w3.org/1999/02/22-rdf-syntax-ns#
rdfs:		http://www.w3.org/2000/01/rdf-schema#

[MappingDeclaration] @collection [[
mappingId	BOOTSTRAPPED-MAPPING-ID1
target		<http://semanticweb.org/spider_flights/airlines/uid={uid}> a <http://semanticweb.org/spider_flights/airlines> ; <http://semanticweb.org/spider_flights/airlines#uid> {uid}^^xsd:integer ; <http://semanticweb.org/spider_flights/airlines#Airline> {Airline}^^xsd:string ; <http://semanticweb.org/spider_flights/airlines#Abbreviation> {Abbreviation}^^xsd:string ; <http://semanticweb.org/spider_flights/airlines#Country> {Country}^^xsd:string . 
source		SELECT * FROM "airlines"

mappingId	BOOTSTRAPPED-MAPPING-ID2
target		<http://semanticweb.org/spider_flights/flights/Airline={Airline};FlightNo={FlightNo}> a <http://semanticweb.org/spider_flights/flights> ; <http://semanticweb.org/spider_flights/flights#Airline> {Airline}^^xsd:integer ; <http://semanticweb.org/spider_flights/flights#FlightNo> {FlightNo}^^xsd:integer ; <http://semanticweb.org/spider_flights/flights#SourceAirport> {SourceAirport}^^xsd:string ; <http://semanticweb.org/spider_flights/flights#DestAirport> {DestAirport}^^xsd:string . 
source		SELECT * FROM "flights"

mappingId	BOOTSTRAPPED-MAPPING-ID3
target		<http://semanticweb.org/spider_flights/flights/Airline={flights_Airline};FlightNo={flights_FlightNo}> <http://semanticweb.org/spider_flights/flights#ref-DestAirport> <http://semanticweb.org/spider_flights/airports/AirportCode={airports_AirportCode}> . 
source		SELECT "flights"."Airline" AS flights_Airline, "flights"."FlightNo" AS flights_FlightNo, "airports"."AirportCode" AS airports_AirportCode FROM "flights", "airports" WHERE "flights"."DestAirport" = "airports"."AirportCode"

mappingId	BOOTSTRAPPED-MAPPING-ID4
target		<http://semanticweb.org/spider_flights/flights/Airline={flights_Airline};FlightNo={flights_FlightNo}> <http://semanticweb.org/spider_flights/flights#ref-SourceAirport> <http://semanticweb.org/spider_flights/airports/AirportCode={airports_AirportCode}> . 
source		SELECT "flights"."Airline" AS flights_Airline, "flights"."FlightNo" AS flights_FlightNo, "airports"."AirportCode" AS airports_AirportCode FROM "flights", "airports" WHERE "flights"."SourceAirport" = "airports"."AirportCode"

mappingId	BOOTSTRAPPED-MAPPING-ID5
target		<http://semanticweb.org/spider_flights/airports/AirportCode={AirportCode}> a <http://semanticweb.org/spider_flights/airports> ; <http://semanticweb.org/spider_flights/airports#City> {City}^^xsd:string ; <http://semanticweb.org/spider_flights/airports#AirportCode> {AirportCode}^^xsd:string ; <http://semanticweb.org/spider_flights/airports#AirportName> {AirportName}^^xsd:string ; <http://semanticweb.org/spider_flights/airports#Country> {Country}^^xsd:string ; <http://semanticweb.org/spider_flights/airports#CountryAbbrev> {CountryAbbrev}^^xsd:string . 
source		SELECT * FROM "airports"
]]

